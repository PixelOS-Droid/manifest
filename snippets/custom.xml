<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="PixelOS"
           fetch="https://github.com/PixelOS-AOSP"
           revision="refs/heads/fourteen" />

  <remote  name="PixelOS-Gitlab"
           fetch="https://gitlab.com/PixelOS-AOSP"
           revision="refs/heads/fourteen" />

  <remote  name="PixelOS-Devices-Gitlab"
           fetch="https://gitlab.com/PixelOS-Devices"
           revision="refs/heads/fourteen" />

  <remote  name="PixelOS-Droid"
           fetch="https://github.com/PixelOS-Droid"
           revision="refs/heads/fourteen-plus" />

  <!-- Rootdir -->
  <project path="bionic" name="bionic" groups="pdk" remote="PixelOS" />

  <!-- Bootable -->
  <project path="bootable/recovery" name="bootable_recovery" groups="pdk" remote="PixelOS" />

  <!-- Build -->
  <project path="build/make" name="build" groups="pdk" remote="PixelOS-Droid">
    <linkfile dest="build/CleanSpec.mk" src="CleanSpec.mk" />
    <linkfile dest="build/buildspec.mk.default" src="buildspec.mk.default" />
    <linkfile dest="build/core" src="core" />
    <linkfile dest="build/envsetup.sh" src="envsetup.sh" />
    <linkfile dest="build/target" src="target" />
    <linkfile dest="build/tools" src="tools" />
  </project>
  <project path="build/soong" name="build_soong" groups="pdk,tradefed" remote="PixelOS">
    <linkfile dest="Android.bp" src="root.bp" />
    <linkfile dest="bootstrap.bash" src="bootstrap.bash" />
  </project>

  <!-- External -->
  <project path="external/jemalloc_new" name="external_jemalloc_new" groups="pdk" remote="PixelOS" />

  <!-- Frameworks -->
  <project path="frameworks/av" name="frameworks_av" groups="pdk" remote="PixelOS-Droid" />
  <project path="frameworks/base" name="frameworks_base" groups="pdk-cw-fs,pdk-fs" remote="PixelOS-Droid" />
  <project path="frameworks/native" name="frameworks_native" groups="pdk" remote="PixelOS-Droid" />
  <project path="frameworks/opt/telephony" name="frameworks_opt_telephony" groups="pdk" remote="PixelOS-Droid" />

  <!-- Hardware -->
  <project path="hardware/google/pixel-sepolicy" name="hardware_google_pixel-sepolicy" groups="generic_fs,pixel" remote="PixelOS" />

  <!-- Packages -->
  <project path="packages/apps/Settings" name="packages_apps_Settings" groups="pdk-fs" remote="PixelOS-Droid" />
  <project path="packages/modules/Bluetooth" name="packages_modules_Bluetooth" groups="pdk" remote="PixelOS" />
  <project path="packages/modules/Connectivity" name="packages_modules_Connectivity" groups="pdk" remote="PixelOS" />
  <project path="packages/services/Telecomm" name="packages_services_Telecomm" remote="PixelOS" />
  <project path="packages/services/Telephony" name="packages_services_Telephony" groups="pdk-cw-fs,pdk-fs" remote="PixelOS-Droid" />

  <!-- System -->
  <project path="system/core" name="system_core" groups="pdk" remote="PixelOS-Droid" />
  <project path="system/sepolicy" name="system_sepolicy" groups="pdk" remote="PixelOS-Droid" />

  <!-- Begin Extras -->

  <!-- Device -->
  <project path="device/lineage/sepolicy" name="device_lineage_sepolicy" remote="PixelOS-Droid" />
  <project path="device/mediatek/sepolicy_vndr" name="device_mediatek_sepolicy_vndr" remote="PixelOS" />

  <!-- Packages -->
  <project path="packages/apps/GameSpace" name="packages_apps_GameSpace" remote="PixelOS-Droid" />
  <project path="packages/apps/CarrierConfig" name="packages_apps_CarrierConfig" remote="PixelOS-Droid" />
  <project path="packages/apps/TouchGestures" name="packages_apps_TouchGestures" remote="PixelOS-Droid" />
  <project path="packages/apps/Updates" name="packages_apps_Updates" remote="PixelOS" />

  <!-- Vendor -->
  <project path="vendor/aosp" name="vendor_aosp" remote="PixelOS-Droid" />
  <project path="vendor/custom-preference" name="vendor_custom-preference" remote="PixelOS" />
  <project path="vendor/microsoft" name="vendor_microsoft" remote="PixelOS" />
  <project path="vendor/gms" name="vendor_gms" clone-depth="1" remote="PixelOS-Devices-Gitlab" />

</manifest>
